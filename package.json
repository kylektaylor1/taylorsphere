{
	"name": "taylorsphere",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"dev:taylor": "dotenv -- turbo dev --filter ./apps/taylorsphere",
		"___ğŸ’½___ DB Scipts ___ğŸ’½___": "",
